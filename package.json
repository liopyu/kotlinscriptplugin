{
  "name": "kotlinscript",
  "displayName": "KotlinScript",
  "description": "Generated KotlinScript typings based on the instance opened.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "type": "module",
  "main": "./out/extension.cjs",
  "contributes": {
    "themes": [
      {
        "label": "KotlinScript Custom Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/kotlinscripttheme.json"
      }
    ],
    "languages": [
      {
        "id": "kotlinscript",
        "aliases": [
          "KotlinScript",
          "Kotlin"
        ],
        "extensions": [
          ".kts"
        ],
        "configuration": "./kotlin.configuration.json"
      },
      {
        "id": "kotlin",
        "aliases": [
          "Kotlin"
        ],
        "extensions": [
          ".kt"
        ],
        "configuration": "./kotlin.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kotlinscript",
        "scopeName": "source.kotlin",
        "path": "./syntaxes/Kotlin.tmLanguage"
      },
      {
        "language": "kotlin",
        "scopeName": "source.kotlin",
        "path": "./syntaxes/Kotlin.tmLanguage"
      }
    ]
  },
  "extensionDependencies": [],
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "build": "rollup --config rollup.config.js"
  },
  "files": [
    "src",
    "resources"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/mocha": "^10.0.9",
    "@types/node": "^20.x",
    "@types/vscode": "^1.95.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "antlr4": "^4.13.2",
    "java-class-reader": "^0.4.3",
    "java-class-tools": "^1.3.2",
    "node-java-maven": "^0.1.2",
    "vscode-languageclient": "^9.0.1"
  }
}